
<div class="container">
  <div class="row mt-5">
    <%= simple_form_for @cocktail do |f| %>
      <%= f.input :name %>
      <%= f.input :photo, label: false %>
      <%= f.input :photo_cache, as: :hidden %>
      <%= f.button :submit, class:'btn btn-ghost btn-sm' %>
    <% end %>
  </div>
</div>
